            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "TZ",
                    "value": "America/New_York.zip"
                },
            ],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "zig-debug-build",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
